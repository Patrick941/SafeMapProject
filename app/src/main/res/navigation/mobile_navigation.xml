<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.mapstemplate.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_history"
        android:name="com.example.mapstemplate.ui.history.HistoryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_Contacts"
        android:name="com.example.mapstemplate.ui.contacts.ContactsFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/nav_my_itineraries"
        android:name="com.example.mapstemplate.ui.current_user_itineraries.CurrentUserItinerariesFragment"
        android:label="My Itineraries"
        tools:layout="@layout/fragment_current_user_itineraries" />

    <fragment
        android:id="@+id/nav_global_itineraries"
        android:name="com.example.mapstemplate.ui.global_itineraries.GlobalItinerariesFragment"
        android:label="Global Itineraries"
        tools:layout="@layout/fragment_global_itineraries" />
</navigation>